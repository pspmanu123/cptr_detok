        "$description": "Switch background active"
      },
      "inactiveHandle": {
        "$type": "color",
        "$value": "#FFFFFF",
        "$description": "Switch handle color in bo"
      },
      "inactiveBackground": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.3",
              "space": "lch"
            }
          }
        },
        "$type": "color",
        "$value": "#BDBDBD",
        "$description": "Switch background inactive"
      }
    },
    "ppt": {
