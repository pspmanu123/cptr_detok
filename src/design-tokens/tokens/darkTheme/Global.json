  "switch": {
    "bo": {
      "inactiveHandle": {
        "$type": "color",
        "$value": "#FFFFFF",
        "$description": "Switch handle color in bo when in inactive"
      },
      "inactiveBackground": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.3",
              "space": "lch"
            }
          }
        },
        "$type": "color",
        "$value": "#BDBDBD",
        "$description": "Switch background inactive in bo"
      },
      "activeHandle": {
        "$type": "color",
        "$value": "#FFFFFF",
        "$description": "Switch handle color in bo when in active"
      },
      "activeBackground": {
        "$type": "color",
        "$value": "{success.main}",
        "$description": "Switch background active in bo"
      }
    },
    "ppt": {
      "inactiveHandle": {
        "$type": "color",
        "$value": "#8C8C8C",
        "$description": "Switch handle color in ppt when in inactive"
      },
      "inactiveBackground": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.3",
              "space": "lch"
            }
          }
        },
        "$type": "color",
        "$value": "#8C8C8C",
        "$description": "Switch background inactive in ppt"
      }
    }
  },
