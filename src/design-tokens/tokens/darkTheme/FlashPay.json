{
  "Primary": {
    "$type": "color",
    "$value": "#12A78A",
    "$description": "FlashPay primary color"
  },
  "Hover": {
    "$type": "color",
    "$value": "#0C7460",
    "$description": "FlashPay primary hover"
  },
  "Dark": {
    "$type": "color",
    "$value": "#055550",
    "$description": "FlashPay primary dark"
  },
  "Header": {
    "$type": "color",
    "$value": "linear-gradient(89.83deg, #FFDA3F 0.11%, #BACB55 19.25%, #46B279 37.87%, #12A78A 58.3%, #0D917F 72.31%, #087A73 99.43%)",
    "$description": "FlashPay primary header"
  },
  "fontFamily": {
    "$type": "fontFamilies",
    "$value": "Manrope",
    "$description": "Flash Pay font family"
  },
  "active": {
    "$type": "border",
    "$value": {
      "color": "{Primary}",
      "width": "1.5"
    },
    "$description": "Flashpay custom button active"
  },
  "tertiary": {
    "$type": "border",
    "$value": {
      "color": "{Primary}",
      "width": "1"
    },
    "$description": "Flashpay tertiary button"
  },
  "tertiaryHover": {
    "$type": "border",
    "$value": {
      "color": "{Hover}",
      "width": "1"
    },
    "$description": "Flashpay tertiary button hover"
  },
  "headerContrast": {
    "$type": "color",
    "$value": "#FFF"
  },
  "textfieldMultiselect": {
    "$type": "border",
    "$value": {
      "width": "1",
      "color": "{Primary}"
    },
    "$description": "Multi-select textfield default border when there's input"
  }
}