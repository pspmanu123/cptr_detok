  "switch": {
    "bo": {
      "activeHandle": {
        "$type": "color",
        "$value": "#FFFFFF",
        "$description": "Switch handle color in bo "
      },
      "activeBG": {
        "$type": "color",
        "$value": "{alert.success.defaultIcon}",
        "$description": "Switch background active"
      },
      "inactiveHandle": {
        "$type": "color",
        "$value": "#FFFFFF",
        "$description": "Switch handle color in bo"
      },
      "inactiveBG": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.3",
              "space": "lch"
            }
          }
        },
        "$type": "color",
        "$value": "#BDBDBD",
        "$description": "Switch background inactive"
      }
    },
    "ppt": {
      "inactiveHandle": {
        "$type": "color",
        "$value": "#737373",
        "$description": "Switch handle color in bo"
      },
      "inactiveBG": {
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.3",
              "space": "lch"
