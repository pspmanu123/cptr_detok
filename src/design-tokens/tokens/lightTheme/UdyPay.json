    "$description": "Udy Pay primary color"
  },
  "Hover": {
    "$type": "color",
    "$value": "#3A1C5F",
    "$description": "Udy Pay primary hover"
  },
  "Dark": {
    "$type": "color",
    "$value": "#542988",
    "$description": "Udy Pay primary dark"
  },
  "Header": {
    "$type": "color",
